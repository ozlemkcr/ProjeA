@model WebApplication2.DAL.uyeTable

@{
    ViewBag.Title = "Üye Sil";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>Üyeyi silmek istediğinize emin misiniz?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label class="control-label col-md-5">Üyenin Adı</label>
        </dt>

        <dd>
            <div class="col-md-6">
                @Html.DisplayFor(model => model.uyeAd)
            </div>
        </dd>

        <dt>
            <label class="control-label col-md-5">Üyenin Soyadı</label>
        </dt>

        <dd>
            <div class="col-md-6">
                @Html.DisplayFor(model => model.uyeSoyad)
            </div>
        </dd>

        <dt>
            <label class="control-label col-md-5">Üyenin E-maili</label>
        </dt>

        <dd>
            <div class="col-md-6">
                @Html.DisplayFor(model => model.uyeEmail)
            </div>
        </dd>

        <dt>
            <label class="control-label col-md-5">Üyenin Şifresi</label>
        </dt>

        <dd>
            <div class="col-md-6">
                @Html.DisplayFor(model => model.uyePass)
            </div>
        </dd>

        <dt>
            <label class="control-label col-md-5">Üyelik Tarihi</label>
        </dt>

        <dd>
            <div class="col-md-6">
                @Html.DisplayFor(model => model.uyelikTarihi)
            </div>
        </dd>

        <dt>
            <label class="control-label col-md-5">Üyelik Değişme Tarihi</label>
        </dt>

        <dd>
            <div class="col-md-6">
                @Html.DisplayFor(model => model.uyelikDegismeTarihi)
            </div>
        </dd>

        <dt>
            <label class="control-label col-md-5">Üyenin Rolü</label>
        </dt>

        <dd>
            <div class="col-md-6">
                @Html.DisplayFor(model => model.uyeRol)
            </div>
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-primary" />
        </div>
    }
</div>
